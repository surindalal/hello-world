pipeline {
	agent any
        stages {
        stage('Checkout') {
            steps {
                echo 'Checkout..'
		checkout scm
            }
        }
        stage('Build') {
            steps {
                echo 'Cleaning..'
		sh 'mvn clean package' 
            }
        }
        stage('Test') {
            steps {
                echo 'Testing....'
		sh 'mvn test'
            }
        }
    }
}
